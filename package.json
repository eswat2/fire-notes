{
  "name": "fire-notes",
  "version": "1.0.0",
  "description": "a simple websocket based notes server...",
  "engineStrict": true,
  "engines": {
    "node": "9.4.0",
    "npm": "5.6.0"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix": "eslint *.js --fix",
    "lintr": "esw *.js --color",
    "watch": "npm run lintr -- --watch",
    "precommit": "lint-staged",
    "report": "eslint *.js -f html > eslint.html"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Richard Hess eswat2@gmail.com (https://about.me/richardhess)",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "eslint": "4.15.0",
    "eslint-find-rules": "3.1.1",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-watch": "3.1.3",
    "husky": "0.14.3",
    "lint-staged": "6.0.0"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "express": "4.16.2",
    "mongoose": "4.13.9",
    "request": "2.83.0",
    "ws": "4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eswat2/fire-notes"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "websockets"
  ],
  "license": "MIT"
}
