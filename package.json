{
  "name": "fire-notes",
  "version": "1.0.0",
  "description": "a simple websocket based notes server...",
  "engineStrict": true,
  "engines": {
    "node": "9.4.0",
    "npm": "5.6.0"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix": "eslint *.js --fix",
    "lintr": "esw *.js --color",
    "watch": "npm run lintr -- --watch",
    "precommit": "lint-staged",
    "report": "eslint *.js -f html > eslint.html"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Richard Hess eswat2@gmail.com (https://about.me/richardhess)",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-find-rules": "3.2.3",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-watch": "3.1.5",
    "husky": "0.14.3",
    "lint-staged": "7.1.2"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "express": "4.16.3",
    "mongoose": "5.1.2",
    "request": "2.87.0",
    "ws": "5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eswat2/fire-notes"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "websockets"
  ],
  "license": "MIT"
}
